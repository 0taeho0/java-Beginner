public class Main2 {

    public static void main(String[] args) {
        String name = "정연택";

        String tc = new String("정연택");

        System.out.println(name == tc);

        String num = "0709201234531";
        char gender = num.charAt(6);
        switch (gender) {
        case 1,3: 
            System.out.println("남자");
            break;
        case 2,4: 
            System.out.println("여자");
            break;
        }
    }

}
