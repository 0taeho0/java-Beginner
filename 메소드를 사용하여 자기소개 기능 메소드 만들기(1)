public class Human {
    // 필드(변수, 속성)
    int height;
    int weight;
    String name;

    Human(int height, int heinght, String name){
        this height = height;
        this weight = weight;
        this name = name;
    }


    // 메소드(함수, 기능)
    void 자기소개() {
        System.out.printf("제 이름은 %s, 키는 %d, "
                + "몸무게는 %d kg입니다.\n",name, height, weight);
    }





}
