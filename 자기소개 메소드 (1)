public class Main2 {

    public static void main(String[] args) {
        String 변수 = "정,연,택";
        String[] arr = 변수.split(",");

        for(int i=0; i<arr.length;i++) {
            System.out.println(arr[i]);
        }

        for(String i : arr) {
            System.out.println(i);
        }

        Human h1 = new Human();
        h1.자기소개();
        h1.height = 168;
        h1.weight = 100;
        h1.name = "정연택";

        h1.자기소개();
    }

}
